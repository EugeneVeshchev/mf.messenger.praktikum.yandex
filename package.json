{
  "name": "homer",
  "version": "0.0.1",
  "description": "Messenger",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build:static": "parcel build --no-scope-hoist 'src/pages/**/*.html' --dist-dir 'static'",
    "local:static": "parcel build --no-scope-hoist 'src/pages/**/*.html'",
    "local:server": "parcel build 'server.ts' --dist-dir './'",
    "start": "yarn local:static && yarn local:server && node server.js"
  },
